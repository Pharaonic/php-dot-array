{
    "name": "pharaonic/php-dot-array",
    "description": "Access array data quickly/easily using dot-notation and asterisk.",
    "keywords": [
        "php",
        "array",
        "php array",
        "php dot array",
        "dot notation",
        "asterisk",
        "pharaonic"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Raggi",
            "email": "raggigroup@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Pharaonic\\DotArray\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pharaonic\\DotArray\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}